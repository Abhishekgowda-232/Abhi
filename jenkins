pipeline {
    agent any
    stages{
        stage('git cloned'){
            steps{
                git url:'https://github.com/Abhishekgowda-232/Abhi.git , branch: "master"
              
            }
        }
        stage('Build docker image'){
            steps{
                script{
                    sh 'docker build -t Abhishekgowda-232/staragileprojectfinance:v1 .'
                    sh 'docker images'
                }
            }
        }
         Stage(Docker login')
        
     stage('Deploy') {
            steps {
                sh 'sudo docker run -itd --name My-first-containe21211 -p 8083:8081 akshu20791/staragileprojectfinance:v1'
                  
                }
            }
        
    }
}
